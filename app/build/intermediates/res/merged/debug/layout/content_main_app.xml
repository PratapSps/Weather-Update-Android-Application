<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="pratap.weatherupdate.MainApp"
    tools:showIn="@layout/activity_main_app"
    android:background="@drawable/mainicon"
    android:gravity="left">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/CITY_NA"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/city"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:autoText="false"
        android:capitalize="words"
        android:singleLine="true"
        android:textColor="#d90404"
        android:textStyle="bold"
        android:width="@dimen/abc_text_size_large_material"
        android:textSize="@dimen/design_navigation_icon_padding"
        android:gravity="center_horizontal"
        android:fontFamily="@string/abc_capital_on"
        />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageupdate"
        android:layout_marginTop="105dp"
        android:src="@drawable/icon1"
        android:layout_below="@+id/currentDate"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/NA"
        android:id="@+id/status"
        android:layout_marginTop="53dp"
        android:fontFamily="@string/abc_capital_on"
        android:gravity="center_horizontal|left"
        android:textStyle="bold"
        android:capitalize="words"
        android:singleLine="false"
        android:textColor="#fba21d"
        android:maxHeight="@dimen/abc_dropdownitem_icon_width"
        android:textSize="@dimen/design_navigation_icon_size"
        android:layout_below="@+id/city"
        android:layout_alignParentStart="true"
        android:layout_above="@+id/DegreeF"
        android:layout_toStartOf="@+id/celsius"
        android:layout_alignEnd="@+id/city" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/NA"
        android:id="@+id/DegreeF"
        android:gravity="center_horizontal"
        android:textStyle="bold"
        android:capitalize="words"
        android:textColor="#fba21d"
        android:textSize="@dimen/abc_alert_dialog_button_bar_height"
        android:layout_alignTop="@+id/imageupdate"
        android:layout_toEndOf="@+id/textView8" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="0"
        android:id="@+id/farenhite"
        android:gravity="bottom"
        android:textStyle="bold"
        android:capitalize="words"
        android:textColor="#e2e54c"
        android:textSize="@dimen/abc_edit_text_inset_top_material"
        android:textIsSelectable="false"
        android:layout_alignTop="@+id/DegreeF"
        android:layout_toEndOf="@+id/DegreeF" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/NA"
        android:id="@+id/degreeC"
        android:gravity="center_horizontal"
        android:textStyle="bold"
        android:capitalize="words"
        android:textColor="#fba21d"
        android:textSize="@dimen/abc_dropdownitem_icon_width"
        android:layout_below="@+id/DegreeF"
        android:layout_alignStart="@+id/DegreeF" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="C"
        android:id="@+id/celsius"
        android:gravity="top"
        android:textStyle="bold"
        android:capitalize="words"
        android:textColor="#e2e54c"
        android:textSize="@dimen/abc_dialog_padding_material"
        android:layout_below="@+id/DegreeF"
        android:layout_toEndOf="@+id/textView2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="F"
        android:id="@+id/textView"
        android:gravity="top"
        android:textStyle="bold"
        android:capitalize="words"
        android:textColor="#e2e54c"
        android:textSize="@dimen/abc_action_button_min_width_overflow_material"
        android:layout_alignTop="@+id/farenhite"
        android:layout_toEndOf="@+id/farenhite" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="0"
        android:id="@+id/textView2"
        android:gravity="bottom"
        android:textStyle="bold"
        android:capitalize="words"
        android:textColor="#e2e54c"
        android:textSize="@dimen/abc_edit_text_inset_top_material"
        android:layout_below="@+id/DegreeF"
        android:layout_toEndOf="@+id/degreeC" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/Min"
        android:id="@+id/textView3"
        android:layout_marginTop="40dp"
        android:textSize="@dimen/abc_select_dialog_padding_start_material"
        android:textColor="#060606"
        android:textStyle="bold"
        android:layout_below="@+id/imageupdate"
        android:layout_alignEnd="@+id/textView4" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/Maxi"
        android:id="@+id/textView4"
        android:layout_below="@+id/textView3"
        android:layout_alignParentStart="true"
        android:textColor="#060606"
        android:textStyle="bold"
        android:textSize="@dimen/abc_select_dialog_padding_start_material"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/Humidity"
        android:id="@+id/textView5"
        android:layout_below="@+id/textView4"
        android:layout_alignParentStart="true"
        android:textColor="#060606"
        android:textStyle="bold"
        android:textSize="@dimen/abc_select_dialog_padding_start_material"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/Wind"
        android:id="@+id/textView6"
        android:layout_below="@+id/textView5"
        android:layout_alignParentStart="true"
        android:textColor="#060606"
        android:textStyle="bold"
        android:textSize="@dimen/abc_select_dialog_padding_start_material"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/NA"
        android:id="@+id/minF"
        android:textColor="#0b49e6"
        android:textStyle="bold"
        android:textSize="@dimen/abc_select_dialog_padding_start_material"
        android:layout_above="@+id/textView4"
        android:layout_toStartOf="@+id/textView8" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/NA"
        android:id="@+id/maxF"
        android:textColor="#0b49e6"
        android:textStyle="bold"
        android:textSize="@dimen/abc_select_dialog_padding_start_material"
        android:layout_below="@+id/minF"
        android:layout_toStartOf="@+id/textView8" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/NA"
        android:id="@+id/humidity"
        android:textColor="#0b49e6"
        android:textStyle="bold"
        android:textSize="@dimen/abc_select_dialog_padding_start_material"
        android:layout_above="@+id/textView6"
        android:layout_toStartOf="@+id/textView8" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/NA"
        android:id="@+id/windspeed"
        android:textColor="#0b49e6"
        android:textStyle="bold"
        android:textSize="@dimen/abc_select_dialog_padding_start_material"
        android:layout_below="@+id/humidity"
        android:layout_alignStart="@+id/humidity" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="MPH"
        android:id="@+id/textView7"
        android:textColor="#0b49e6"
        android:textStyle="bold"
        android:textSize="@dimen/abc_select_dialog_padding_start_material"
        android:layout_below="@+id/humidity"
        android:layout_alignEnd="@+id/celsius" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="%"
        android:id="@+id/textView8"
        android:textColor="#0b49e6"
        android:textStyle="bold"
        android:textSize="@dimen/abc_select_dialog_padding_start_material"
        android:layout_above="@+id/textView7"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Clouds  :"
        android:id="@+id/textView9"
        android:textColor="#060606"
        android:textStyle="bold"
        android:textSize="@dimen/abc_select_dialog_padding_start_material"
        android:layout_below="@+id/textView6"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/NA"
        android:id="@+id/clouds"
        android:textColor="#0b49e6"
        android:textStyle="bold"
        android:textSize="@dimen/abc_select_dialog_padding_start_material"
        android:layout_below="@+id/windspeed"
        android:layout_alignStart="@+id/windspeed" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="%"
        android:id="@+id/textView11"
        android:textColor="#0b49e6"
        android:textStyle="bold"
        android:textSize="@dimen/abc_select_dialog_padding_start_material"
        android:layout_below="@+id/textView7"
        android:layout_toEndOf="@+id/clouds" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/Sunrise"
        android:id="@+id/textView10"
        android:textColor="#060606"
        android:textStyle="bold"
        android:textSize="@dimen/abc_select_dialog_padding_start_material"
        android:layout_below="@+id/textView9"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/Sunset"
        android:id="@+id/textView12"
        android:textColor="#060606"
        android:textStyle="bold"
        android:textSize="@dimen/abc_select_dialog_padding_start_material"
        android:layout_below="@+id/textView10"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/NA"
        android:id="@+id/sunrise"
        android:textColor="#0b49e6"
        android:textStyle="bold"
        android:textSize="@dimen/abc_dialog_padding_top_material"
        android:layout_alignBottom="@+id/textView10"
        android:layout_toEndOf="@+id/textView5"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/NA"
        android:id="@+id/sunset"
        android:textColor="#0b49e6"
        android:textStyle="bold"
        android:textSize="@dimen/abc_dialog_padding_top_material"
        android:layout_alignBottom="@+id/textView12"
        android:layout_toEndOf="@+id/textView5"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/currentDate"
        android:layout_below="@+id/city"
        android:layout_alignEnd="@+id/city"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:gravity="right"
        android:textSize="@dimen/abc_floating_window_z"
        android:textColor="#fb9110"
        android:textStyle="bold"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:id="@+id/Message"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/textView3"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:gravity="left"
        android:textColor="#fefefc"
        android:textSize="@dimen/abc_dialog_padding_top_material"
        />
</RelativeLayout>
